<%= simple_form_for(@section) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :position %>
    <%= f.input :title %>
    <%= f.input :text %>
    <%= f.input :photos, as: :attachinary %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
